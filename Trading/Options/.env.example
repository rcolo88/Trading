# API Keys and Secrets
# Copy this file to .env and fill in your actual keys

# Schwab API (for future live/paper trading)
SCHWAB_APP_KEY=your_app_key_here
SCHWAB_APP_SECRET=your_app_secret_here
SCHWAB_REDIRECT_URI=https://localhost:8080

# Polygon.io (if upgrading to paid data in the future)
POLYGON_API_KEY=your_polygon_api_key_here

# QuantConnect (if using their platform)
QUANTCONNECT_USER_ID=your_user_id_here
QUANTCONNECT_API_TOKEN=your_api_token_here
